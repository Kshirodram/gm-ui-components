{
  "name": "gm-ui-components",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "color": "^2.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-responsive": "^1.3.4",
    "react-test-renderer": "^16.0.0",
    "styled-components": "^2.1.2"
  },
  "files": [
    "index.js",
    "es",
    "umd",
    "gm-ui-components.min.js"
  ],
  "scripts": {
    "test": "jest --watchAll --env=jsdom",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build": "node ./tools/build.js",
    "watch": "export BABEL_ENV=umd && NODE_ENV=development && rollup -c -f umd -o index.js -w",
    "prepublish": "node ./tools/build.js",
    "clean": "git clean -fdX .",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.10",
    "@storybook/addon-info": "^3.2.11",
    "@storybook/addon-knobs": "^3.2.10",
    "@storybook/addon-links": "^3.2.10",
    "@storybook/react": "^3.2.10",
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.9",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "gzip-size": "^4.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jest-enzyme": "^4.0.0",
    "jest-junit": "^3.1.0",
    "jest-styled-components": "^4.6.0",
    "pretty-bytes": "^4.0.2",
    "react-addons-test-utils": "^15.6.2",
    "regenerator-runtime": "^0.11.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupTests.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|png|svg|ttf)$": "<rootDir>/config/emptyModule.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ]
  }
}
